echo
echo
echo
echo "$0"
echo
echo
./surfboard-checkmha
echo 'WCR avg history (49 tests @ b=3): 0.460816 ->0.460204 ->0.46102 ->0.459796' | tee -a Corduroy.log
echo | tee -a Corduroy.log
