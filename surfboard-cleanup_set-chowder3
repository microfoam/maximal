#!/bin/bash
echo >> Corduroy.log
echo '                                               < eat chowder 3: last six nudgings from cinch-k all k-mers>' >> Corduroy.log

answer="n"

while getopts c option
do
case "${option}"
in
c) 	./surfboard-Pop_Up
	;;
esac
done

if [ -f Surf_wavereport.mha ]; then
	    rm Surf_wavereport.mha
fi


	./maximal -aKR -D 8 waves/chowder/s32-k.txt
	./maximal -aKR -D 8 waves/chowder/s40-k.txt
	./maximal -aKR -D 8 waves/chowder/s42-k.txt
	./maximal -aKR -D 8 waves/chowder/s44-k.txt
	./maximal -aKR -D 8 waves/chowder/str06-k.txt
	./maximal -aKR -D 8 waves/chowder/stri04-k.txt

./surfboard-checkmha
echo 'Previous commit WCR averages of 6, allowing all k-mers in cinch-k: 0.455 (6 nudgings)' | tee -a Corduroy.log
echo | tee -a Corduroy.log
