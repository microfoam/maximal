	;;
esac
done

if [ -f Surf_wavereport.mha ]; then
	    rm Surf_wavereport.mha
fi

	# conflict begins in cinch-t

	# conflict begins in cinch-k
	./maximal -KR -D 8 waves/chowder/nugget-00007.txt	

	# solved
	./maximal -KR -D 8 waves/chowder/nugget-42356.txt	# solved-13	
	./maximal -KR -D 8 waves/chowder/nugget-47633.txt	# solved-14 cinch-k x_history propagation
	./maximal -KR -D 8 waves/chowder/nugget-50546.txt	# solved-14 cinch-k x_history propagation
	./maximal -KR -D 8 waves/chowder/nugget-00002.txt	# solved-15 delete unnecessary cinch-k block blocking needed cinch
	./maximal -KR -D 8 waves/chowder/nugget-00001.txt	# solved-16 less strict entry into split fractal block in mark_tela
	./maximal -KR -D 8 waves/chowder/nugget-00003.txt	# solved-16 less strict entry into split fractal block in mark_tela
	./maximal -KR -D 8 waves/chowder/nugget-00004.txt	# solved-16 less strict entry into split fractal block in mark_tela
