#!/bin/bash
./surfboard-Pop_Up

if [ -f Surf_wavereport.mha ]; then
	rm Surf_wavereport.mha
fi
if [ -f waves/foam_and_waves/foam_and_chowder.log ]; then
	rm waves/foam_and_waves/foam_and_chowder.log
fi

./maximal -Rnx  waves/tubespit/tubespit-59-tcycle.txt
./maximal -Rnx  waves/tubespit/tubespit_v344_22.txt
./maximal -Rnxc waves/tubespit/tubespit_v343_23.txt
./maximal -Rnxc waves/tubespit/tubespit_v344_01.txt

./surfboard-checkmha


