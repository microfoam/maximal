#!/bin/bash
echo >> Corduroy.log
echo '                                               < sand_dunes >' >> Corduroy.log

while getopts c option
do
case "${option}"
in
c) 	echo 'with fresh compile (-c)' >> Corduroy.log
	./surfboard-Pop_Up
	;;
esac
done

if [ -f Surf_wavereport.mha ]; then
        rm Surf_wavereport.mha
fi


# DIAGNOSTIC TESTS START ON LINE 21
./maximal -DKRn  waves/tubespit/seq-017-variant.txt
./maximal -DKRn  waves/tubespit/seq-017-cycle5.txt
./maximal -DKRnC waves/tubespit/seq-017-cycle5.txt
./maximal -DKRn  waves/tubespit/seq-154-v427_cycABs.txt
./maximal -DKR   waves/tubespit/seq-000-orig_b.txt 
./maximal -DKRn  waves/tubespit/seq-000-orig_c.txt 
./maximal -DKR   waves/tubespit/seq-000-orig_d.txt 
./maximal -DKR   waves/tubespit/seq-000-orig_e.txt 
./maximal -DKR   waves/tubespit/seq-000-orig_f.txt 
./maximal -DKRx  waves/tubespit/seq-033-imprf1.txt 
./maximal -DKR   waves/tubespit/seq-100-v343_10.txt 
./maximal -DKRn  waves/chowder/seq6-koslip-snippet.txt 
./maximal -DKRn  waves/chowder/abba_zabba_1.txt 
./maximal -DKRn  waves/chowder/abba_zabba_2.txt 
./maximal -DKRn  waves/chowder/abba_zabba_3.txt 
./maximal -DKRn  waves/chowder/abba_zabba_4.txt 
./maximal -DKR   waves/chowder/seq26-snippet.txt 
./maximal -DKR   waves/chowder/gal11-knot1.txt 
./maximal -DKR   waves/chowder/seq26-variations1.txt 
./maximal -DKR   waves/chowder/seq26-variations2.txt 
./maximal -DKRn  waves/chowder/jetsam-01-snippet.txt 
./maximal -DKRn  waves/chowder/jetsam-03-snippet.txt 

./surfboard-checkmha
echo 'sand_dunes benchmark WCR average of 22: 0.378318' | tee -a Corduroy.log
echo | tee -a Corduroy.log
