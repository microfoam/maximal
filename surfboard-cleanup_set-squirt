#!/bin/bash
echo >> Corduroy.log
echo '                                               < squirt chowder >' >> Corduroy.log

while getopts c option
do
case "${option}"
in
c) 	./surfboard-Pop_Up
	grep 'define b' src/microhom-skor.h | tee -a Corduroy.log
	;;
esac
done

if [ -f Surf_wavereport.mha ]; then
	    rm Surf_wavereport.mha
fi


	# cinch-t originating 2-D conflict

	# cinch-k originating 2-D conflict

	# solved: push new solved runs onto top of stack starting w/ solved-64 
	./maximal -KnxR waves/squeegee/squirt_03-b6.txt		# solved-66 mark_tela edits; b4-6
	./maximal -KnxR waves/squeegee/squirt_02-b6.txt		# solved-65 delete all_S condition in split fractals module of mark_tela(); b3-6
	./maximal -KnxR waves/squeegee/squirt_01-b6.txt		# solved-64 new module in mark_tela concerning cycling islands w/ perfect and imperfect columns b3-6

echo
echo
echo
echo "$0"
echo
echo
./surfboard-checkmha
echo 'Commit WCR average of 3 (b=6|5): 0.496667 (chowder-free)' | tee -a Corduroy.log
echo 'Commit WCR average of 3 (b=4  ): 0.5      (chowder-free)' | tee -a Corduroy.log
echo 'Commit WCR average of 3 (b=3  ): 0.48     (chowder-free)' | tee -a Corduroy.log
echo | tee -a Corduroy.log
