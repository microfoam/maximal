#!/bin/bash
echo >> Corduroy.log
echo '                                               < squirt chowder >' >> Corduroy.log

while getopts c option
do
case "${option}"
in
c) 	./surfboard-Pop_Up
	;;
esac
done

if [ -f Surf_wavereport.mha ]; then
	    rm Surf_wavereport.mha
fi

	# cinch-t originating 2-D conflict
	./maximal -KnxR -D 8 waves/squeegee/squirt_08-b6.txt		# 

	# cinch-k originating 2-D conflict
	./maximal -KnxR -D 8 waves/squeegee/squirt_09-b6.txt		# 

	# solved: push new solved runs onto top of stack starting w/ solved-64 
	./maximal -KnxR waves/squeegee/squirt_07-b6.txt		# solved-70 muting deprecated code in mark_tela(); b3--6
	./maximal -KnxR waves/squeegee/squirt_06-b6.txt		# solved-69 sub-threshold fractals condition to skip if next parent column is perfect k; b3--6
	./maximal -KnxR waves/squeegee/squirt_05-b6.txt		# solved-68 mark_tela edits continued again; b4--6
	./maximal -KnxR waves/squeegee/squirt_04-b6.txt		# solved-67 mark_tela edits continued; b3--6
	./maximal -KnxR waves/squeegee/squirt_03-b6.txt		# solved-66 mark_tela edits; b4-6
	./maximal -KnxR waves/squeegee/squirt_02-b6.txt		# solved-65 delete all_S condition in split fractals module of mark_tela(); b3-6
	./maximal -KnxR waves/squeegee/squirt_01-b6.txt		# solved-64 new module in mark_tela concerning cycling islands w/ perfect and imperfect columns b3-6

echo
echo
echo
echo "$0"
echo
echo
./surfboard-checkmha
echo 'Commit WCR average of 7 (b=6): 0.511429 (chowder-free)' | tee -a Corduroy.log
echo 'Commit WCR average of 7 (b=6): 0.515714 (chowder-free)' | tee -a Corduroy.log
echo 'Commit WCR average of 7 (b=4): 0.51     (chowder-free)' | tee -a Corduroy.log
echo 'Commit WCR average of 7 (b=3): 0.478571 (chowder-free)' | tee -a Corduroy.log
echo | tee -a Corduroy.log
