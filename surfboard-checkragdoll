clear
echo 'Running surfboard-check-ragdoll'
echo
echo 'Number of tests initiated:'
grep -c waves Surf_wavereport.log
echo
echo 'Number of tests with perfect final passes (no conflicts in columns):'
grep -cE '\t1000' Surf_wavereport.mha 
echo
echo 'Number of tests with recovered 1-D sequences:'
grep -cE 'C.1000' Surf_wavereport.mha 
echo
echo 'Number of tests canceled:'
grep -cE 'Canceled' Surf_wavereport.mha 
echo
echo 'Number of tests nudgelized of type 1:'
grep -cE 't=3' Surf_wavereport.mha 
echo
echo 'Number of tests nudgelized of type 2:'
grep -cE 't=4' Surf_wavereport.mha 
echo
echo 'Number of tests with check-transitions warnings:'
grep -cE 'checktransits' Surf_wavereport.mha 
echo
echo 'Drosophila melanogaster vnd exon three Fisher-Yates randomized:'
grep -c ex3_Dmela Surf_wavereport.mha
