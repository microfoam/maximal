echo 
echo 'M.'
echo '\\H.                  _     _     _     _     _     _     _     _     _     _     _'
echo '\\\\A.________.~._~.~.) `-._) `-._) `-._) `-._) `-._) `-._) `-._) `-._) `-._) `-._) `-._'
echo '``````````````'
gcc -o maximal -Wall -Wextra -Wstrict-prototypes -O2 mha_v4.27.c -lm
